{
  "version": 2,
  "builds": [
    { "src": "./backend/server.js", "use": "@vercel/node" },
    { "src": "./frontend/build", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "/backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/build/index.html"
    }
  ]
}
